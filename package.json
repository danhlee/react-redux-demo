{
  "name": "react-redux-demo",
  "version": "1.0.0",
  "description": "react/redux demo app",
  "scripts": {
    "start": "run-p start:dev start:api",
    "start:dev": "webpack-dev-server --mode development --config webpack.config.dev.js --port 3000 --progress --profile --color",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "test": "jest --watch",
    "test:ci": "jest",
    "clean:build": "rm -rf ./build && mkdir build",
    "prebuild": "run-p clean:build test:ci",
    "build": "webpack --mode production --config webpack.config.prod.js --progress --profile --color",
    "postbuild": "run-p start:api serve:build",
    "serve:build": "http-server ./build"
  },
  "author": "danhlee",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "bootstrap": "^4.4.1",
    "immer": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.18.3",
    "fetch-mock": "^9.1.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.13.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
